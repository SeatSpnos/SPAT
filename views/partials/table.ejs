<div class="container-fluid">
  <div class="page-header text-center">
    <div class="container">
      <table class="table" id="table">  
        <thead>
          <tr>
            <% for (var i = 0; i < colName.length ; i++){ %>
              <th><%= colName[i] %></th>
            <% } %>
          </tr>
        </thead>      
        <tbody>       
          <% for (var i = begin ; i < end ; i++) { %>
            <tr id = <%= searchResult[i][keys[0]]%>>
            <% for (var j =1 ; j < colSize ; j++) { %>
              <td> <p><%= searchResult[i][keys[j]] %> </p></td>
            <% } %>
            </tr>
          <% } %>                 
        </tbody>
      </table>
    </div>
  </div>
</div>

<style>
  table {
    word-wrap: break-word;
}
 td {max-width: 120px;white-space: nowrap;overflow: hidden;text-overflow: ellipsis;}
</style>
  