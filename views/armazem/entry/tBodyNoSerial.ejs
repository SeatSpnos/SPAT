<tr>
	<td><%= item.referencia %></td>
	<td><%= item.nome %></td>		
	<td><input class="form-hidden itemQuantity" type="number" name="<%= item.nome %>" ref="<%= item.referencia %>" value="<%= quantity %>"></td>
	<td><input class="form-hidden itemObs" type="text" name="<%= item.nome %>Obs" ref="<%= item.referencia %>"></td>
	<td><a href="#" title="Remover Item" data-toggle="tooltip_removeItem" class="removeItem"><i class="fa fa-minus" aria-hidden="true"></i></a></td>
</tr>